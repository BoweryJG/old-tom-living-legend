# Copy this file to .env.local and fill in your values

# Supabase (Optional - app works without it)
REACT_APP_SUPABASE_URL=your_supabase_url
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key

# OpenAI API (Optional - for AI features)
REACT_APP_OPENAI_API_KEY=your_openai_api_key

# ElevenLabs (Optional - for voice synthesis)
REACT_APP_ELEVENLABS_API_KEY=your_elevenlabs_api_key
REACT_APP_ELEVENLABS_VOICE_ID=your_voice_id

# Higgs Audio (Optional - for advanced audio)
REACT_APP_HIGGS_API_KEY=your_higgs_api_key
