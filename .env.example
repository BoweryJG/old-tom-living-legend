# API Configuration
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here
REACT_APP_OPENAI_BASE_URL=https://api.openai.com/v1
REACT_APP_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Environment
REACT_APP_ENV=development
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_VOICE_RECOGNITION=true
REACT_APP_ENABLE_SPATIAL_AUDIO=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ENABLE_ANALYTICS=false

# Performance Settings
REACT_APP_MAX_TEXTURE_SIZE=2048
REACT_APP_TARGET_FPS=60
REACT_APP_MEMORY_THRESHOLD=500

# Debug Settings (development only)
REACT_APP_DEBUG_ANIMATIONS=false
REACT_APP_DEBUG_AUDIO=false
REACT_APP_DEBUG_PERFORMANCE=false